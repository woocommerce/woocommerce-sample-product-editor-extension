{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "extension/my-extension-name",
	"version": "0.1.0",
	"title": "My Extension Name",
	"category": "widgets",
	"icon": "flag",
	"description": "A block to demonstrate extending the Product Editor",
	"attributes": {
		"message": {
			"type": "string",
			"__experimentalRole": "content",
			"source": "text",
			"selector": "div"
		}
	},
	"supports": {
		"html": false,
		"inserter": false
	},
	"textdomain": "my-extension-name",
	"editorScript": "file:./index.js"
}
